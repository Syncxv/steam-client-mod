{
	"compilerOptions": {
		"jsx": "react",
		"jsxFactory": "window.steamed.Webpack.Common.React.createElement",
		"jsxFragmentFactory": "window.steamed.Webpack.Common.React.Fragment",
		"target": "ES2020",
		"module": "commonjs",
		"lib": [
			"dom",
			"es6",
			"es2021",
			"esnext.asynciterable"
		],
		"skipLibCheck": true,
		"sourceMap": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"outDir": "./dist",
		"moduleResolution": "node",
		"removeComments": true,
		"noImplicitAny": false,
		"allowJs": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"resolveJsonModule": true,
		"baseUrl": "./src",
		"noEmit": false,
		"paths": {
			"@api/*": [
				"./api/*"
			],
			"@api": [
				"./api"
			],
			"@components/*": [
				"./components/*"
			],
			"@components": [
				"./components"
			],
			"@utils/*": [
				"./utils/*"
			],
			"@utils": [
				"./utils"
			],
			"@webpack/common": [
				"./webpack/common"
			],
			"@webpack": [
				"./webpack"
			],
			"@src/types/*": [
				"./types/*"
			],
			"@src/types": [
				"./types"
			],
		}
	},
	"exclude": [
		"node_modules"
	],
	// "include": ["./**/*.tsx", "./**/*.ts", "./**/*.js", "./**/*.jsx"]
	"include": [
		"./**/*"
	]
}